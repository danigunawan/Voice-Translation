<div class='row'>
  Create Command:
  <%= form_for command, method: :post, remote: true do |f|%>
    <div class='form-group'>
      <%= f.label :description %>
      <%= f.text_field :description, class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label :keywords %>
      <%= f.text_field :keywords, class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label :response %>
      <%= f.text_area :response, class: 'form-control', rows: '10' %>
    </div>
    <%= f.submit "Save Command", class: 'btn btn-primary' %>
  <% end %>
</div>
